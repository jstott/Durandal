{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["src/durandal/js", "src/plugins/js", "src/transitions/js"],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "tools/jsdoc/templates/default",
        "encoding": "utf8",
        "destination": "dist/apidocs/",
        "recurse": true
    }
}